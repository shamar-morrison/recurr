{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "noImplicitAny": false,
    "jsx": "react-native",
    "paths": {
      "@/*": [
        "./*"
      ],
      "firebase/auth": [
        "./node_modules/@firebase/auth/dist/rn/index.rn.d.ts"
      ],
      "tailwind.config": [
        "./tailwind.config.js"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}